{
  "name": "fairtrack",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged --concurrent false",
    "db:generate": "pnpm drizzle-kit generate --config=./src/server/db/drizzle.config.ts",
    "db:migrate": "pnpm drizzle-kit migrate --config=./src/server/db/drizzle.config.ts",
    "db:push": "pnpm drizzle-kit push --config=./src/server/db/drizzle.config.ts",
    "db:clean": "tsx src/server/db/seed/clean.ts --env=development",
    "db:seed": "tsx src/server/db/seed/index.ts --env=development",
    "db:seed:dev": "tsx src/server/db/seed/index.ts --env=development",
    "db:seed:demo": "tsx src/server/db/seed/index.ts --env=demo",
    "db:seed:clean": "tsx src/server/db/seed/index.ts --env=development --clean",
    "db:seed:clean:dev": "tsx src/server/db/seed/index.ts --env=development --clean",
    "db:seed:clean:demo": "tsx src/server/db/seed/index.ts --env=demo --clean",
    "prepare": "husky",
    "test": "vitest",
    "test:dev": "NEXT_PUBLIC_ENV=testing pnpm dev -p 3001",
    "test:dev:e2e": "NEXT_PUBLIC_ENV=testing concurrently \"pnpm dev -p 3001\" \"wait-on http://localhost:3001 && cypress open --e2e\" --names \"server,cypress\" --prefix-colors \"blue,green\"",
    "test:e2e": "NEXT_PUBLIC_ENV=testing cypress run",
    "test:e2e:headed": "NEXT_PUBLIC_ENV=testing cypress run --headed"
  },
